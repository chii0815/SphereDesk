            {{ dump() }}
        {% block body %}

        {% endblock %}
        </div>
        <script src="{{ asset('js/jquery-3.3.1.min.js') }}"></script>
        <script src="{{ asset('js/bootstrap.bundle.min.js') }}"></script>
        <script src="{{ asset('js/all.min.js') }}"></script>
        <script src="{{ asset('js/nav.clock.js') }}"></script>
        {% block javascripts %}

        {% endblock %}
